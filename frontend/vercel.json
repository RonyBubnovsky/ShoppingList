{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!assets/|favicon.ico|robots.txt|manifest.webmanifest|service-worker.js|sw.js|sitemap.xml|apple-touch-icon.png|icon-.*\\.png|.*\\.(css|js|mjs|map|png|jpg|jpeg|gif|svg|webp|ico|json|txt)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}